{
	"apiVersion": "v1",
	"kind": "List",
	"items": [{
		"apiVersion": "extensions/v1beta1",
		"kind": "ThirdPartyResource",
		"description": "Managed MongoDB Replica-Set",
		"metadata": {
			"name": "mongodb.operator.kubestack.com"
		},
		"versions": [{
			"name": "v1"
		}]
	}, {
		"apiVersion": "v1",
		"kind": "Namespace",
		"metadata": {
			"name": "kubestack"
		}
	}, {
		"apiVersion": "extensions/v1beta1",
		"kind": "Deployment",
		"metadata": {
			"labels": {
				"operator": "mongodb.operator.kubestack.com",
				"heritage": "kubestack.com"
			},
			"name": "mongodb-operator",
			"namespace": "kubestack"
		},
		"spec": {
			"replicas": 1,
			"selector": {
				"matchLabels": {
					"operator": "mongodb.operator.kubestack.com"
				}
			},
			"template": {
				"metadata": {
					"labels": {
						"operator": "mongodb.operator.kubestack.com",
						"heritage": "kubestack.com"
					}
				},
				"spec": {
					"containers": [{
						"image": "quay.io/kubestack/mongodb-operator:latest",
						"name": "mongodb-operator",
						"resources": {
							"limits": {
								"cpu": "200m",
								"memory": "100Mi"
							},
							"requests": {
								"cpu": "100m",
								"memory": "50Mi"
							}
						}
					}]
				}
			}
		}
	}, {
		"apiVersion": "operator.kubestack.com/v1",
		"kind": "Mongodb",
		"metadata": {
			"name": "mongo",
			"namespace": "default"
		},
		"spec": {
			"mongodb": {
				"replicas": 3,
				"mongodb_limit_cpu": "100m",
				"mongodb_limit_memory": "64Mi"
			}
		}
	}]
}
